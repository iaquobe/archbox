### Settings from original zathurarc ###
set font "monospace normal 16"
set statusbar-basename true

### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#cacdc9'
set completion-bg '#0f0f0e'

# Command line completion group elements
set completion-group-fg '#5C5D52'
set completion-group-bg '#0f0f0e'

# Current command line completion element
set completion-highlight-fg '#0f0f0e'
set completion-highlight-bg '#cacdc9'

# Default foreground/background color
set default-bg rgba(15,15,14,0.9)

# Input bar
set inputbar-fg '#cacdc9'
set inputbar-bg '#0f0f0e'

# Notification
set notification-fg '#cacdc9'
set notification-bg '#0f0f0e'

# Error notification
set notification-error-fg '#cacdc9'
set notification-error-bg '#2E4B54'

# Warning notification
set notification-warning-fg '#cacdc9'
set notification-warning-bg '#2E4B54'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#cacdc9'
set statusbar-bg '#0f0f0e'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#5C5D52'
set highlight-active-color '#5C5D52'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#cacdc9'

# 'Loading...' text
set render-loading-fg '#cacdc9'
set render-loading-bg '#0f0f0e'

# Index mode
set index-fg '#cacdc9'
set index-bg '#0f0f0e'

# Selected element in index mode
set index-active-fg '#0f0f0e'
set index-active-bg '#cacdc9'